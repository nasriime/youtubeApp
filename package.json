{
  "name": "youtube-app",
  "version": "1.0.0",
  "description": "youtube app",
  "main": "index.js",
  "scripts": {
    "build": "browserify -t babelify ./app/app.js -o ./app.js",
    "watch": "watchify -t babelify ./app/app.js -o ./app.js --debug",
    "dev": "lite-server",
    "serve": "npm run dev && npm run watch"
  },
  "dependencies": {
    "angular": "^1.5.7",
    "angular-ui-router": "^1.0.0-alpha.5"
  },
  "devDependencies": {
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "babelify": "^6.1.2",
    "browserify": "^13.0.1",
    "watchify": "^3.7.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^4.0.2",
    "gulp-sass": "^4.0.1",
    "lite-server": "^2.3.0"
  }
}
